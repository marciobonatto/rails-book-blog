<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= post.author %>
  </p>

  <p>
    <strong>Book rating:</strong>
    <%= post.book_rating %>
  </p>

  <p>
    <strong>Book description:</strong>
    <%= post.book_description %>
  </p>

</div>
